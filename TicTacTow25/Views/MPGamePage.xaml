<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="TicTacTow25.Views.MPGamePage"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:vm="clr-namespace:TicTacTow25.ViewModels"
    
    Title="MPGamePage"
    x:DataType="vm:MPGamePageVM">
    <VerticalStackLayout>
        <Grid x:Name="grdMain" />
        <Label
            HorizontalOptions="Center"
            Text="{Binding MyName}"
            VerticalOptions="End" />
    </VerticalStackLayout>
</ContentPage>